{
  "name": "keycloak-js",
  "version": "12.0.0-SNAPSHOT",
  "description": "Keycloak Adapter",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "build": "yarn build:lib && yarn build:es",
    "build:lib": "rimraf lib && tsc && copyfiles --flat src/index.d.ts lib",
    "build:es": "rimraf es && tsc --project tsconfig.es.json && copyfiles --flat src/index.d.ts es"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keycloak/keycloak",
    "directory": "adapters/oidc/js/packages/keycloak-js"
  },
  "author": "Keycloak",
  "license": "Apache-2.0",
  "homepage": "https://www.keycloak.org",
  "keywords": [
    "keycloak",
    "sso",
    "oauth",
    "oauth2",
    "authentication"
  ],
  "dependencies": {
    "base64-js": "1.3.1",
    "js-sha256": "0.9.0"
  }
}
